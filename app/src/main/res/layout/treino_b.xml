<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="416dp"
        android:layout_height="1323dp"
        android:layout_marginTop="75dp"
        android:orientation="vertical"
        android:padding="9dp"
        app:layout_constraintTop_toTopOf="@+id/nav_1">

        <Space
            android:layout_width="match_parent"
            android:layout_height="15dp" />

        <androidx.gridlayout.widget.GridLayout
            android:layout_width="match_parent"
            android:layout_height="522dp">

            <ImageButton
                android:id="@+id/b1_img"
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:layout_column="0"
                app:layout_row="0"
                app:srcCompat="@drawable/b1" />

            <TextView
                android:id="@+id/b1_txt"
                android:layout_width="222dp"
                android:layout_height="wrap_content"
                android:text="Remada Pulley: 4 Sérires 12 Repetições"
                app:layout_column="1"
                app:layout_row="0" />

            <CheckBox
                android:id="@+id/b1_check"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Feito"
                app:layout_column="2"
                app:layout_row="0" />

            <ImageButton
                android:id="@+id/b2_img"
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:layout_column="0"
                app:layout_row="1"
                app:srcCompat="@drawable/b2" />

            <CheckBox
                android:id="@+id/b2_check"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Feito"
                app:layout_column="2"
                app:layout_row="1" />

            <TextView
                android:id="@+id/b2_txt"
                android:layout_width="222dp"
                android:layout_height="wrap_content"
                android:text="Remada Baixa: 4 Séries 12 Repetições"
                app:layout_column="1"
                app:layout_row="1" />

            <ImageButton
                android:id="@+id/b3_img"
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:layout_column="0"
                app:layout_row="2"
                app:srcCompat="@drawable/b3" />

            <CheckBox
                android:id="@+id/b3_check"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Feito"
                app:layout_column="2"
                app:layout_row="2" />

            <TextView
                android:id="@+id/b3_txt"
                android:layout_width="222dp"
                android:layout_height="wrap_content"
                android:text="Remada: 4 Séries 12 Repetições"
                app:layout_column="1"
                app:layout_row="2" />

            <ImageButton
                android:id="@+id/b4_img"
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:layout_column="0"
                app:layout_row="3"
                app:srcCompat="@drawable/b4" />

            <CheckBox
                android:id="@+id/b4_check"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Feito"
                app:layout_column="2"
                app:layout_row="3" />

            <TextView
                android:id="@+id/b4_txt"
                android:layout_width="222dp"
                android:layout_height="wrap_content"
                android:text="Remada Alta: 4 Séries 12 Repetições"
                app:layout_column="1"
                app:layout_row="3" />

            <ImageButton
                android:id="@+id/b5_img"
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:layout_column="0"
                app:layout_row="4"
                app:srcCompat="@drawable/b5" />

            <CheckBox
                android:id="@+id/b5_check"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Feito"
                app:layout_column="2"
                app:layout_row="4" />

            <TextView
                android:id="@+id/b5_txt"
                android:layout_width="222dp"
                android:layout_height="wrap_content"
                android:text="Rosca Direta: 3 Séries 12 Repetições"
                app:layout_column="1"
                app:layout_row="4" />

            <ImageButton
                android:id="@+id/b6_img"
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:layout_column="0"
                app:layout_row="5"
                app:srcCompat="@drawable/b6" />

            <CheckBox
                android:id="@+id/b6_check"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Feito"
                app:layout_column="2"
                app:layout_row="5" />

            <TextView
                android:id="@+id/b6_txt"
                android:layout_width="222dp"
                android:layout_height="wrap_content"
                android:text="Rosca Barra W: 3 Séries 12 Repetições"
                app:layout_column="1"
                app:layout_row="5" />

            <ImageButton
                android:id="@+id/b7_img"
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:layout_column="0"
                app:layout_row="6"
                app:srcCompat="@drawable/b7" />

            <CheckBox
                android:id="@+id/b7_check"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Feito"
                app:layout_column="2"
                app:layout_row="6" />

            <TextView
                android:id="@+id/b7_txt"
                android:layout_width="222dp"
                android:layout_height="wrap_content"
                android:text="Abdominal: 4Séries 12 Repetições"
                app:layout_column="1"
                app:layout_row="6" />
        </androidx.gridlayout.widget.GridLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="30dp" />

        <Button
            android:id="@+id/Submeter_b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Submeter" />
    </LinearLayout>

    <FrameLayout
        android:id="@+id/nav_1"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:background="#830000"
        android:padding="9dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/voltar_TreinoB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|start"
            android:text="Voltar"
            android:textColor="#FFFFFF" />
        <ImageView
            android:id="@+id/usuario_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            app:srcCompat="@drawable/usuario" />
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>